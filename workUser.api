type (
	CreateUserReq {
		Name string `json:"name"`
		Pwd  string `json:"name"`
	}

	CreateUserResp {
		Code    int    `json:"code"`
		Message string `json:"message"`
		Data    string `json:"data"`
	}
)

type Request {
	Name string `path:"name,options=you|me"`
}

type Response {
	Code    int    `json:"code"`
	Message string `json:"message"`
	Data    string `json:"data"`
}

service workUser-api {
	@handler CreateUser
	post /user/create(CreateUserReq) returns(CreateUserResp)
}